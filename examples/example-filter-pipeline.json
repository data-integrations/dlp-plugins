            {
                "name": "Call Center Records",
                "plugin": {
                    "name": "GCSFile",
                    "type": "batchsource",
                    "label": "Call Center Records",
                    "artifact": {
                        "name": "google-cloud",
                        "version": "0.13.2-SNAPSHOT",
                        "scope": "USER"
                    },
                    "properties": {
                        "project": "auto-detect",
                        "format": "text",
                        "serviceFilePath": "auto-detect",
                        "filenameOnly": "false",
                        "recursive": "false",
                        "schema": "{\"type\":\"record\",\"name\":\"etlSchemaBody\",\"fields\":[{\"name\":\"offset\",\"type\":\"long\"},{\"name\":\"body\",\"type\":\"string\"}]}",
