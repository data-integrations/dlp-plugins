        "driverResources": {
            "memoryMB": 2048,
            "virtualCores": 1
        },
        "postActions": [],
        "properties": {},
        "processTimingEnabled": true,
        "stageLoggingEnabled": false,
        "engine": "spark",
        "schedule": "0 * * * *",
        "maxConcurrentRuns": 1
    }
}